{
  "scripts": {
    "start": "npm run docker-compose-up && nodemon server.js",
    "docker-compose-up": "docker compose -f ./docker-compose.yml -p shadowbound-be up -d",
    "docker-compose-down": "docker compose -f ./docker-compose.yml -p shadowbound-be down",
    "docker-compose-logs": "docker compose -f ./docker-compose.yml -p shadowbound-be logs",
    "docker-compose-ps": "docker compose -f ./docker-compose.yml -p shadowbound-be ps"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "uuid-random": "^1.3.2",
    "ws": "^8.18.0"
  }
}
